<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DoctorAppointment</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/telerik/CBMS_Theme.css" asp-append-version="true" />

    <link rel="stylesheet" href="~/DoctorAppointment.styles.css" asp-append-version="true" />

    <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
    <script src="~/lib/telerik/kendo.all.min.js"></script>
    <script src="~/lib/telerik/kendo.aspnetmvc.min.js"></script>
</head>
<body>
        <nav class="navbar">
            <ul class="navbar-nav">
            <li class="navbar__item">

                    <a class="navbar__link" asp-area="" asp-controller="Home" asp-action="Index"><i class="bi bi-house"></i><span>Home</span></a>
                        </li>
                <li class="navbar__item">
                    <a class="navbar__link" asp-area="" asp-controller="Home" asp-action="About"><i class="bi bi-shield-check"></i><span>Privacy</span></a>
                        </li>
                <partial name="_LoginPartial.cshtml" />
                    </ul>
                   
        </nav>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
